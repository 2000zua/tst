generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id        String   @id @default(uuid())
  frst_name String
  last_name String
  email     String
  username  String
  birthday  DateTime
  createAt  DateTime @default(now())
  updatedAt DateTime @updatedAt
}

